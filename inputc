#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1998f20 .scope module, "testConditioner" "testConditioner" 2 5;
 .timescale 0 0;
v0x19b2d20_0 .var "clk", 0 0;
v0x19b2dc0_0 .net "conditioned", 0 0, v0x19b2820_0; 1 drivers
v0x19b2e70_0 .net "falling", 0 0, v0x19b2960_0; 1 drivers
v0x19b2f20_0 .var "pin", 0 0;
v0x19b3000_0 .net "rising", 0 0, v0x19b2ab0_0; 1 drivers
S_0x1999010 .scope module, "dut" "inputconditioner" 2 13, 3 8, S_0x1998f20;
 .timescale 0 0;
P_0x1999108 .param/l "counterwidth" 3 17, +C4<0101>;
P_0x1999130 .param/l "waittime" 3 18, +C4<01010>;
v0x1975a50_0 .net "clk", 0 0, v0x19b2d20_0; 1 drivers
v0x19b2820_0 .var "conditioned", 0 0;
v0x19b28c0_0 .var "counter", 4 0;
v0x19b2960_0 .var "negativeedge", 0 0;
v0x19b2a10_0 .net "noisysignal", 0 0, v0x19b2f20_0; 1 drivers
v0x19b2ab0_0 .var "positiveedge", 0 0;
v0x19b2b90_0 .var "synchronizer0", 0 0;
v0x19b2c30_0 .var "synchronizer1", 0 0;
E_0x19a0540 .event posedge, v0x1975a50_0;
    .scope S_0x1999010;
T_0 ;
    %set/v v0x19b28c0_0, 0, 5;
    %end;
    .thread T_0;
    .scope S_0x1999010;
T_1 ;
    %set/v v0x19b2b90_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x1999010;
T_2 ;
    %set/v v0x19b2c30_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x1999010;
T_3 ;
    %ix/load 0, 1, 0;
    %assign/v0 v0x19b2820_0, 0, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v0x19b2ab0_0, 0, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v0x19b2960_0, 0, 0;
    %end;
    .thread T_3;
    .scope S_0x1999010;
T_4 ;
    %wait E_0x19a0540;
    %ix/load 0, 1, 0;
    %assign/v0 v0x19b2ab0_0, 0, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v0x19b2960_0, 0, 0;
    %load/v 8, v0x19b2820_0, 1;
    %load/v 9, v0x19b2c30_0, 1;
    %cmp/u 8, 9, 1;
    %jmp/0xz  T_4.0, 4;
    %ix/load 0, 5, 0;
    %assign/v0 v0x19b28c0_0, 0, 0;
    %jmp T_4.1;
T_4.0 ;
    %load/v 8, v0x19b28c0_0, 5;
    %mov 13, 0, 1;
    %cmpi/u 8, 10, 6;
    %jmp/0xz  T_4.2, 4;
    %ix/load 0, 5, 0;
    %assign/v0 v0x19b28c0_0, 0, 0;
    %load/v 8, v0x19b2c30_0, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v0x19b2820_0, 0, 8;
    %load/v 8, v0x19b2c30_0, 1;
    %jmp/0xz  T_4.4, 8;
    %ix/load 0, 1, 0;
    %assign/v0 v0x19b2ab0_0, 0, 1;
    %jmp T_4.5;
T_4.4 ;
    %ix/load 0, 1, 0;
    %assign/v0 v0x19b2960_0, 0, 1;
T_4.5 ;
    %jmp T_4.3;
T_4.2 ;
    %load/v 8, v0x19b28c0_0, 5;
    %mov 13, 0, 27;
    %addi 8, 1, 32;
    %ix/load 0, 5, 0;
    %assign/v0 v0x19b28c0_0, 0, 8;
T_4.3 ;
T_4.1 ;
    %load/v 8, v0x19b2a10_0, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v0x19b2b90_0, 0, 8;
    %load/v 8, v0x19b2b90_0, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v0x19b2c30_0, 0, 8;
    %jmp T_4;
    .thread T_4;
    .scope S_0x1998f20;
T_5 ;
    %set/v v0x19b2d20_0, 1, 1;
    %end;
    .thread T_5;
    .scope S_0x1998f20;
T_6 ;
    %delay 10, 0;
    %load/v 8, v0x19b2d20_0, 1;
    %inv 8, 1;
    %set/v v0x19b2d20_0, 8, 1;
    %jmp T_6;
    .thread T_6;
    .scope S_0x1998f20;
T_7 ;
    %vpi_call 2 26 "$dumpfile", "inputconditioner.vcd";
    %vpi_call 2 27 "$dumpvars", 1'sb0, S_0x1999010;
    %vpi_call 2 31 "$display", "Clock Pin | Conditioned Rising Falling";
    %set/v v0x19b2f20_0, 0, 1;
    %delay 5, 0;
    %set/v v0x19b2f20_0, 1, 1;
    %delay 5, 0;
    %set/v v0x19b2f20_0, 0, 1;
    %delay 5, 0;
    %set/v v0x19b2f20_0, 1, 1;
    %delay 5, 0;
    %vpi_call 2 35 "$display", "%b   %b | %b   %b   %b", v0x19b2d20_0, v0x19b2f20_0, v0x19b2dc0_0, v0x19b3000_0, v0x19b2e70_0;
    %vpi_call 2 36 "$display", $time;
    %set/v v0x19b2f20_0, 0, 1;
    %delay 40, 0;
    %set/v v0x19b2f20_0, 1, 1;
    %delay 40, 0;
    %vpi_call 2 40 "$display", "%b   %b | %b   %b   %b", v0x19b2d20_0, v0x19b2f20_0, v0x19b2dc0_0, v0x19b3000_0, v0x19b2e70_0;
    %vpi_call 2 41 "$display", $time;
    %set/v v0x19b2f20_0, 1, 1;
    %delay 4000, 0;
    %set/v v0x19b2f20_0, 0, 1;
    %delay 400, 0;
    %vpi_call 2 45 "$display", "%b   %b | %b   %b   %b", v0x19b2d20_0, v0x19b2f20_0, v0x19b2dc0_0, v0x19b3000_0, v0x19b2e70_0;
    %vpi_call 2 46 "$display", $time;
    %delay 1000, 0;
    %vpi_call 2 48 "$finish";
    %end;
    .thread T_7;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "inputconditioner.t.v";
    "./inputconditioner.v";
