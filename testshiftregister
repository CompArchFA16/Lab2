L_0x1d18260 .part v0x1d17c00_0, 7, 1;
    .scope S_0x1cecb00;
T_0 ;
    %wait E_0x1cd1b40;
    %load/v 8, v0x1d17930_0, 1;
    %jmp/0xz  T_0.0, 8;
    %load/v 8, v0x1d177f0_0, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v0x1d17c00_0, 0, 8;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0x1d179e0_0, 1;
    %jmp/0xz  T_0.2, 8;
    %load/v 8, v0x1d17c00_0, 8;
    %ix/load 0, 1, 0;
    %mov 4, 0, 1;
    %shiftl/i0  8, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v0x1d17c00_0, 0, 8;
    %load/v 8, v0x1d17a80_0, 1;
    %ix/load 0, 1, 0;
    %ix/load 1, 0, 0;
    %assign/v0/x1 v0x1d17c00_0, 0, 8;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x1cd1d20;
T_1 ;
    %delay 1000, 0;
    %load/v 8, v0x1d17cf0_0, 1;
    %inv 8, 1;
    %set/v v0x1d17cf0_0, 8, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x1cd1d20;
T_2 ;
    %vpi_call 2 33 "$dumpfile", "shiftregister.vcd";
    %vpi_call 2 34 "$dumpvars";
    %vpi_call 2 36 "$display", " clk  |   pclk   |  pload |     pin      |  sin  |      pout     |  sout  |  expected";
    %set/v v0x1d17cf0_0, 1, 1;
    %set/v v0x1d17fd0_0, 0, 1;
    %set/v v0x1d17ef0_0, 0, 1;
    %movi 8, 16, 8;
    %set/v v0x1d17d90_0, 8, 8;
