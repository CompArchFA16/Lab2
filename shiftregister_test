#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x13f5730 .scope module, "testshiftregister" "testshiftregister" 2 7;
 .timescale 0 0;
v0x1442b40_0 .var "clk", 0 0;
v0x1442be0_0 .var "parallelDataIn", 7 0;
v0x1442c90_0 .net "parallelDataOut", 7 0, v0x14426e0_0; 1 drivers
v0x1442d40_0 .var "parallelLoad", 0 0;
v0x1442e20_0 .var "peripheralClkEdge", 0 0;
v0x1442ed0_0 .var "serialDataIn", 0 0;
v0x1442f50_0 .net "serialDataOut", 0 0, v0x14429b0_0; 1 drivers
S_0x13f5820 .scope module, "dut" "shiftregister" 2 18, 3 9, S_0x13f5730;
 .timescale 0 0;
P_0x142d078 .param/l "width" 3 10, +C4<01000>;
v0x142ae60_0 .net "clk", 0 0, v0x1442b40_0; 1 drivers
v0x1442640_0 .net "parallelDataIn", 7 0, v0x1442be0_0; 1 drivers
v0x14426e0_0 .var "parallelDataOut", 7 0;
v0x1442780_0 .net "parallelLoad", 0 0, v0x1442d40_0; 1 drivers
v0x1442830_0 .net "peripheralClkEdge", 0 0, v0x1442e20_0; 1 drivers
v0x14428d0_0 .net "serialDataIn", 0 0, v0x1442ed0_0; 1 drivers
v0x14429b0_0 .var "serialDataOut", 0 0;
v0x1442a50_0 .var "shiftregistermem", 7 0;
E_0x1408950 .event posedge, v0x142ae60_0;
    .scope S_0x13f5820;
T_0 ;
    %wait E_0x1408950;
    %load/v 8, v0x1442830_0, 1;
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_0.0, 4;
    %load/v 16, v0x14428d0_0, 1;
    %mov 8, 16, 1;
    %load/v 16, v0x1442a50_0, 7; Select 7 out of 8 bits
    %mov 9, 16, 7;
    %set/v v0x1442a50_0, 8, 8;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0x1442780_0, 1;
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_0.2, 4;
    %load/v 8, v0x1442640_0, 8;
    %set/v v0x1442a50_0, 8, 8;
T_0.2 ;
T_0.1 ;
    %ix/load 1, 7, 0;
    %mov 4, 0, 1;
    %jmp/1 T_0.4, 4;
    %load/x1p 8, v0x1442a50_0, 1;
    %jmp T_0.5;
T_0.4 ;
    %mov 8, 2, 1;
T_0.5 ;
; Save base=8 wid=1 in lookaside.
    %set/v v0x14429b0_0, 8, 1;
    %load/v 8, v0x1442a50_0, 8;
    %set/v v0x14426e0_0, 8, 8;
    %jmp T_0;
    .thread T_0;
    .scope S_0x13f5730;
T_1 ;
    %vpi_call 2 28 "$display", "-------------------------";
    %vpi_call 2 29 "$display", "SHIFT REGISTER TEST CASES";
    %vpi_call 2 30 "$display", "-------------------------";
    %vpi_call 2 33 "$display", "peripheralClkEdge parallelLoad | parallelDataIn serialDataIn | parallelDataOut serialDataOut";
    %set/v v0x1442e20_0, 1, 1;
    %set/v v0x1442d40_0, 0, 1;
    %set/v v0x1442be0_0, 0, 8;
    %set/v v0x1442ed0_0, 0, 1;
    %vpi_call 2 35 "$display", " %b  %b  |  %b  %b  |  %b  %b ", v0x1442e20_0, v0x1442d40_0, v0x1442be0_0, v0x1442ed0_0, v0x1442c90_0, v0x1442f50_0;
    %vpi_call 2 38 "$display", "peripheralClkEdge parallelLoad | parallelDataIn serialDataIn | parallelDataOut serialDataOut";
    %set/v v0x1442e20_0, 1, 1;
    %set/v v0x1442d40_0, 1, 1;
    %set/v v0x1442be0_0, 0, 8;
    %set/v v0x1442ed0_0, 0, 1;
    %vpi_call 2 40 "$display", " %b  %b  |  %b  %b  |  %b  %b ", v0x1442e20_0, v0x1442d40_0, v0x1442be0_0, v0x1442ed0_0, v0x1442c90_0, v0x1442f50_0;
    %vpi_call 2 43 "$display", "peripheralClkEdge parallelLoad | parallelDataIn serialDataIn | parallelDataOut serialDataOut";
    %set/v v0x1442e20_0, 1, 1;
    %set/v v0x1442d40_0, 1, 1;
    %set/v v0x1442be0_0, 0, 8;
    %set/v v0x1442ed0_0, 0, 1;
    %vpi_call 2 45 "$display", " %b  %b  |  %b  %b  |  %b  %b ", v0x1442e20_0, v0x1442d40_0, v0x1442be0_0, v0x1442ed0_0, v0x1442c90_0, v0x1442f50_0;
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "shiftregister.t.v";
    "./shiftregister.v";
